{
    "printWidth": 80,
    "tabWidth": 2,
    "use Tabs": false,
    "semi": true,
    "editor.formatOnSave": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "prosewrap": "never",
    "htmlwhitespaceSensitivity": "ignore"
}